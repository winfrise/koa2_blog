<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <% include ../public/head %>
</head>

<body>
    <% include ../public/header %>

                 

    <div class="container">
        <div class="row">
            <div class="col-md-3 col-sm-4 col-xs-12">
                
    <div class="sub-nav-box">
        <div class="sub-nav-title clearfix">
            视频图片
        </div>
        <div class="sub-nav" id="subNav">
            <ul>
                <% child_categories.forEach(item=> { %>
                <li><a href="/article/list?id=<%=item.id%>"><%=item.name%></a></li>
                <% }) %>
            </ul>
        </div>
    </div>
    <script type="text/javascript">
        var $subNav = $("#subNav");
        $subNav.children("ul").find("li").InitNav(4337);//初始化响应式菜单
    </script>
            </div>
            <div class="col-md-9 col-sm-8 col-xs-12">
                <div class="all-Breadcrumb">
                    <div class="float-left visible-md hidden-sm hidden-xs"><%=current_category.name%></div>
                    <div class="float-right">


    <ul class="breadcrumb">
        <li><a href="/"><i class="fa fa-home"></i><span> 首页</span></a></li>
        <li><a href="#"><%=current_category.name %></a></li>
    </ul>

    </div>
                </div>
                <div class="product-list images-list imgae-same-size margin-top-20">
                    <% if(articles.length > 0) { %>
                    <ul class="marginAuto-15">
                        <% articles.forEach(item=> { %>
                        <li class="col-md-6 col-sm-6 col-xs-6 margin-bottom-20 item text-center">
                            <a href="/article/list?id=<%=item.id%>">
                                <div class="boxs">
                                    <div class="imgHover"><img src="<%=item.image_url%>"></div>
                                    <div class="title">
                                        <%=item.name%>
                                    </div>
                                </div>
                            </a>
                        </li>
                        <% }) %>
                    </ul>
                    <% } else {%>
                        <div class="no-data">暂无数据</div>
                    <% } %>
                </div>
                
        <div class="text-center padding-20">
            <ul class="pagination">
                <li class="disabled"><a>&laquo;</a></li>
                <li class=active><a href="/tppd/">1</a></li>
                <li class="disabled"><a>&raquo;</a></li>
            </ul>
        </div>
    
            </div>
        </div>
    </div>

    <% include ../public/footer %>
</body>

</html>